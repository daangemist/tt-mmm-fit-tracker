{
  "name": "teletron-extension-fit-tracker",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev:build": "vite build",
    "dev": "nodemon --ignore dist -e ts,svelte --watch src --watch ../src --exec \"npm run dev:build\"",
    "dev:server": "nodemon -e js,ts,json --watch ./dist --watch ./tests/server --exec \"npm run dev:server:run\"",
    "dev:server:run": "ts-node ./tests/server/cli.ts extension ./package.json"
  },
  "devDependencies": {
    "@teletron/types": "^0.12.0",
    "@types/express": "^4.17.21",
    "@types/yargs": "^17.0.32",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "sqlite": "^4.2.1",
    "sqlite3": "^5.1.6",
    "supersave": "^0.14.2",
    "yargs": "^17.7.2"
  },
  "main": "backend.js",
  "teletron": {
    "name": "fit-tracker",
    "displayName": "Fit Tracker",
    "assets": [
      "dist/fit-tracker.js",
      "dist/fit-tracker.js.map",
      "dist/fit-tracker.css"
    ],
    "collections": [
      {
        "name": "weight",
        "relations": [],
        "filterSortFields": {
          "when": "string"
        }
      }
    ]
  }
}
